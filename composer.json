{
    "name": "prokki/htpasswd",
    "type": "symfony-bundle",
    "description": "This symfony user provider reads user from the htpasswd file.",
    "homepage": "https://github.com/prokki/htpasswd",
    "keywords": [
        "htpasswd",
        "basic",
        "authentication",
        "md5",
        "crypt",
        "bcrypt",
        "blowfish",
        "apache",
        "password"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Falko Matthies",
            "email": "falko.m@web.de"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/password-hasher": "^6.0",
        "symfony/security-core": "^6.0",
        "whitehat101/apr1-md5": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Prokki\\Htpasswd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Prokki\\Htpasswd\\Tests\\": "tests/"
        }
    }
}
